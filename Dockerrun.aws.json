{
  "containerDefinitions": [
    {
      "essential": true,
      "memory": 128,
      "image": "postgres",
      "mountPoints": [
        {
          "containerPath": "/var/lib/postgresql/data/",
          "sourceVolume": "Db"
        }
      ],
      "name": "db",
      "portMappings": [
        {
          "containerPort": 5432,
          "hostPort": 5432
        }
      ]
    },
    {
      "environment": [
        {
          "name": "CHOKIDAR_USEPOLLING",
          "value": "true"
        }
      ],
      "essential": true,
      "memory": 128,
      "image": "front_end",
      "name": "front_end",
      "mountPoints": [
        {
          "containerPath": "/app",
          "sourceVolume": "_Front-End"
        },
        {
          "containerPath": "/app/node_modules",
          "sourceVolume": "AppNode_Modules"
        }
      ],
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    },
    {
      "command": [
        "--connection",
        "${DATABASE_URL}",
        "--port",
        "5433",
        "--schema",
        "public",
        "--append-plugins",
        "postgraphile-plugin-connection-filter,@graphile-contrib/pg-simplify-inflector",
        "--cors"
      ],
      "essential": true,
      "memory": 128,
      "image": "graphql",
      "name": "graphql",
      "portMappings": [
        {
          "containerPort": 5433,
          "hostPort": 5433
        }
      ]
    }
  ],
  "family": "",
  "volumes": [
    {
      "host": {
        "sourcePath": "db"
      },
      "name": "Db"
    },
    {
      "host": {
        "sourcePath": "./front-end"
      },
      "name": "_Front-End"
    },
    {
      "host": {
        "sourcePath": "/app/node_modules"
      },
      "name": "AppNode_Modules"
    }
  ]
}